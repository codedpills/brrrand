{
  "name": "brrrand",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite",
    "dev:demo": "VITE_DEMO_MODE=true vite",
    "dev:real": "VITE_DEMO_MODE=false vite",
    "build": "tsc -b && vite build",
    "build:demo": "VITE_DEMO_MODE=true tsc -b && vite build",
    "build:real": "VITE_DEMO_MODE=false tsc -b && vite build",
    "build:pages": "npm run build",
    "build:pages:demo": "npm run build:demo",
    "build:pages:real": "npm run build:real",
    "build:server": "tsc -p server/tsconfig.json",
    "build:worker": "tsc -p tsconfig.worker.json",
    "deploy": "npm run build && wrangler deploy",
    "deploy:demo": "VITE_DEMO_MODE=true npm run build && wrangler deploy",
    "deploy:real": "VITE_DEMO_MODE=false npm run build && wrangler deploy",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "server": "node dist/server/index.js",
    "dev:server": "nodemon --watch 'server/**/*.ts' --exec 'ts-node server/index.ts'",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "start": "NODE_ENV=production npm run build && npm run build:server && npm run server",
    "cf:dev": "wrangler dev",
    "cf:deploy": "npm run deploy"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router": "^1.127.3",
    "@tanstack/router-devtools": "^1.127.3",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@cloudflare/workers-types": "^4.20250712.0",
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.19.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "wrangler": "^4.24.3"
  }
}
